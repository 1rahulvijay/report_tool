# Environment
ENVIRONMENT=development

# Database Configuration
DB_ENGINE=oracledb

# Oracle Connection Credentials (REQUIRED)
ORACLE_USER=my_database_user
ORACLE_PASSWORD=my_secure_password
ORACLE_DSN=my_oracle_host:1521/my_service_name

# Oracle Pool settings
ORACLE_MIN_POOL=5
ORACLE_MAX_POOL=20

# Oracle Schema Filtering
ORACLE_SCHEMA_FILTER=MY_SCHEMA

# Security
ALLOWED_ORIGINS=https://mycompany.com,https://reports.internal
